{
  "name": "univer-sheet-data-form",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "description": "Data form plugin for univer sheets.",
  "author": "iamyunsin<yunsin@vip.qq.com>",
  "license": "Apache-2.0",
  "keywords": [
    "univerjs",
    "data form"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "./lib/cjs/index.js",
    "module": "./lib/es/index.js",
    "exports": {
      ".": {
        "import": "./lib/es/index.js",
        "require": "./lib/cjs/index.js",
        "types": "./lib/types/index.d.ts"
      },
      "./*": {
        "import": "./lib/es/*",
        "require": "./lib/cjs/*",
        "types": "./lib/types/index.d.ts"
      }
    }
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "dev": "tsc & vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "@univerjs/core": "*",
    "@univerjs/design": "*",
    "@univerjs/ui": "*",
    "@wendellhu/redi": ">=0.12.13",
    "clsx": ">=2.0.0",
    "dnd-core": "^14.0.1",
    "react": ">=16.9.0",
    "react-arborist": "^3.4.0",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "@types/react-dom": "^18.2.19",
    "@univerjs/core": "0.1.0-beta.5",
    "@univerjs/design": "0.1.0-beta.5",
    "@univerjs/icons": "^0.1.29",
    "@univerjs/ui": "0.1.0-beta.5",
    "@wendellhu/redi": "^0.12.13",
    "clsx": "^2.1.0",
    "dnd-core": "^14.0.1",
    "react": "^18.2.0",
    "react-arborist": "^3.4.0",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@univerjs/core": "*",
    "@univerjs/design": "*",
    "@univerjs/docs": "0.1.0-beta.5",
    "@univerjs/docs-ui": "0.1.0-beta.5",
    "@univerjs/engine-formula": "0.1.0-beta.5",
    "@univerjs/engine-numfmt": "0.1.0-beta.5",
    "@univerjs/engine-render": "0.1.0-beta.5",
    "@univerjs/network": "0.1.0-beta.5",
    "@univerjs/rpc": "0.1.0-beta.5",
    "@univerjs/sheets": "0.1.0-beta.5",
    "@univerjs/sheets-formula": "0.1.0-beta.5",
    "@univerjs/sheets-numfmt": "0.1.0-beta.5",
    "@univerjs/sheets-ui": "0.1.0-beta.5",
    "@univerjs/ui": "*",
    "@univerjs/uniscript": "0.1.0-beta.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^13.6.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-externals": "^0.6.2",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
